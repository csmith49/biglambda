<data input="Review" key="String" output="Int">
    <weight name="hasAttr" val=10>
    <weight name="goodForKids" val=10>
    <weight name="bool_to_int" val=10>
    <example>
        <input>
            ("Madison", "WI", "eh", 4, {})
            ("Granbury", "TX", "bad", 2, {'Good for Kids': True})
            ("Madison", "WI", "great", 1, {'Good for Kids': False})
            ("Dallas", "TX", "ew", 10, {})
            ("Dallas", "TX", "eh", 2, {'Good for Kids': True})
        </input>
        <output>
            ("Madison", 0)
            ("Granbury", 1)
            ("Dallas", 0)
        </output>
    </example>
</data>
