<data input="String" output="Int" key="String">
    <weight name="find_matches" val="100" />
    <weight name="fst" val="0.0001" />
    <weight name="snd" val="0.0001" />
    <example>
        <input>
            "retweet #foo #bar"
            "stuff #bar#foo#baz"
            "#baz#baz why not #baz"
        </input>
        <output>
            ("#foo", 2)
            ("#bar", 2)
            ("#baz", 4)
        </output>
    </example>
</data>
