<data input="Int" output="Int">
    <weight name="add" val="100">
    <norm name="add" tag="ac">
    <example>
        <input delim=",">1,2,3</input>
        <output val=6>
    </example>
    <example>
        <input delim=",">4,5,6</input>
        <output val=15>
    </example>
</data>
<trs>
    <rules>
        add(zero, 1) -> 1
        add(1, zero) -> 1
        add(add(1, 2), 3) -> add(1, add(2, 3))
        square(zero) -> zero
        fadd(zero, 1) -> 1
        fadd(1, zero) -> 1
        fadd(fadd(1, 2), 3) -> fadd(1, fadd(2, 3))
        round(cast(1)) -> 1
    </rules>
</trs>