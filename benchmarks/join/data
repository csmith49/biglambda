    <example>
        <input>
            ("DB0", [("K0", "V0"), ("K2", "V2")])
            ("DB1", [("K1", "V3")])
            ("DB2", [("K1", "V4")])
        </input>
        <output>
            ("K0", [("K0", "V0")])
            ("K1", [("K1", "V3"), ("K1", "V4")])
            ("K2", [("K2", "V2")])
        </output>
    </example>
</data>
